# docker-compose tags #
    REPO=onlyoffice
    STATUS=""
    TAG_SRV=1.0.0
    TAG_MYSQL=8.0.18
    ELK_VERSION=7.8.1
    CONTAINER_PRIFIX=onlyoffice

# zookeeper #
    ZOO_PORT=2181
    ZOO_HOST=${CONTAINER_PRIFIX}-zookeeper
    ZOO_SERVER=server.1=${ZOO_HOST}:2888:3888

# kafka #
    KAFKA_HOST=${CONTAINER_PRIFIX}-kafka
    KAFKA_ADVERTISED_LISTENERS=LISTENER_DOCKER_INTERNAL://${KAFKA_HOST}:9092
    KAFKA_LISTENER_SECURITY_PROTOCOL_MAP=LISTENER_DOCKER_INTERNAL:PLAINTEXT,LISTENER_DOCKER_EXTERNAL:PLAINTEXT
    KAFKA_INTER_BROKER_LISTENER_NAME=LISTENER_DOCKER_INTERNAL
    KAFKA_ZOOKEEPER_CONNECT=${ZOO_HOST}:2181
    KAFKA_BROKER_ID=1
    KAFKA_LOG4J_LOGGERS=kafka.controller=INFO,kafka.producer.async.DefaultEventHandler=INFO,state.change.logger=INFO
    KAFKA_OFFSETS_TOPIC_REPLICATION_FACTOR=1

# elasticsearch #
    ELK_HOST=${CONTAINER_PRIFIX}-elasticsearch

# app service environment #
    SERVICE_PORT=5050
    APP_DOTNET_ENV=test
    APP_CORE_BASE_DOMAIN=localhost
    APP_CORE_MACHINEKEY=your_core_machinekey

    DOCUMENT_SERVER_JWT_SECRET=your_jwd_secret_value
    DOCUMENT_SERVER_JWT_HEADER=AuthorizationJwt
    DOCUMENT_SERVER_URL_PUBLIC=/ds-vpath/
    DOCUMENT_SERVER_URL_CONVERTER=/ds-vpath/ConvertService.ashx
    DOCUMENT_SERVER_URL_INTERNAL=http://onlyoffice-document-server/
    DOCUMENT_SERVER_HOST=${CONTAINER_PRIFIX}-document-server
    
    MYSQL_ROOT_PASSWORD=my-secret-pw
    MYSQL_DATABASE=onlyoffice
    MYSQL_USER=onlyoffice_user
    MYSQL_PASSWORD=onlyoffice_pass
    MYSQL_HOST=${CONTAINER_PRIFIX}-mysql-server

# service host #
    API_HOST=${CONTAINER_PRIFIX}-api
    API_SYSTEM_HOST=${CONTAINER_PRIFIX}-api_system
    URLSHORTENER_HOST=${CONTAINER_PRIFIX}-urlshortener
    STUDIO_NOYIFY_HOST=${CONTAINER_PRIFIX}-studio.notify
    NOTIFY_HOST=${CONTAINER_PRIFIX}-notify
    PEOPLE_SERVER_HOST=${CONTAINER_PRIFIX}-people.server
    FILES_HOST=${CONTAINER_PRIFIX}-files
    FILES_SERVICES_HOST=${CONTAINER_PRIFIX}-files_services
    STUDIO_HOST=${CONTAINER_PRIFIX}-studio
    BACKUP_HOST=${CONTAINER_PRIFIX}-backup
    PROXY_HOST=${CONTAINER_PRIFIX}-proxy

# proxy upstream environment #
    SERVICE_API="${API_HOST:-api}:${SERVICE_PORT:-5050}"
    SERVICE_API_SYSTEM="${API_SYSTEM_HOST:-api_system}:${SERVICE_PORT:-5050}"
    SERVICE_URLSHORYENER="${URLSHORTENER_HOST:-urlshortener}:${SERVICE_PORT:-5050}"
    SERVICE_STUDIO_NOTIFY="${STUDIO_NOYIFY_HOST:-studio.notify}:${SERVICE_PORT:-5050}"
    SERVICE_PEOPLE_SERVER="${PEOPLE_SERVER_HOST:-people.server}:${SERVICE_PORT:-5050}"
    SERVICE_FILES="${FILES_HOST:-files}:${SERVICE_PORT:-5050}"
    SERVICE_FILES_SERVICES="${FILES_SERVICES_HOST:-files_services}:${SERVICE_PORT:-5050}"
    SERVICE_STUDIO="${STUDIO_HOST:-studio}:${SERVICE_PORT:-5050}"
    SERVICE_BACKUP="${BACKUP_HOST:-backup}:${SERVICE_PORT:-5050}"
    DOCUMENT_SERVER="${DOCUMENT_SERVER_HOST:-document-server}"
    SERVICE_PORT=${SERVICE_PORT:-5050}

    NETWORK_NAME=onlyoffice